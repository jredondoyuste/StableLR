Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"BreitWigner", "[", 
    RowBox[{"\[CapitalOmega]_", ",", "pars_", ",", 
     RowBox[{"rmax_", ":", "25"}], ",", 
     RowBox[{"prec_", ":", "18"}]}], "]"}], ":=", " ", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"sol", ",", "ns", ",", " ", "a", ",", "b"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"sol", "=", 
       RowBox[{"solIN", "[", 
        RowBox[{"rmax", ",", "\[CapitalOmega]", ",", "pars", ",", 
         SuperscriptBox["10", 
          RowBox[{"-", "3"}]], ",", "prec"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"ns", " ", "=", " ", 
       RowBox[{"First", "@", 
        RowBox[{"NSolve", "[", 
         RowBox[{
          RowBox[{"{", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{
             RowBox[{"Re", "[", 
              RowBox[{"sol", "[", "rmax", "]"}], "]"}], "==", " ", 
             RowBox[{
              RowBox[{"a", " ", 
               RowBox[{"Cos", "[", 
                RowBox[{"\[CapitalOmega]", " ", "rmax"}], "]"}]}], " ", "+", 
              " ", 
              RowBox[{"b", " ", 
               RowBox[{"Sin", "[", 
                RowBox[{"\[CapitalOmega]", " ", "rmax"}], "]"}]}]}]}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"Re", "[", 
              RowBox[{
               RowBox[{"sol", "'"}], "[", "rmax", "]"}], "]"}], "==", " ", 
             RowBox[{
              RowBox[{
               RowBox[{"-", "\[CapitalOmega]"}], " ", "a", " ", 
               RowBox[{"Sin", "[", 
                RowBox[{"\[CapitalOmega]", " ", "rmax"}], "]"}]}], " ", "+", 
              " ", 
              RowBox[{"b", " ", "\[CapitalOmega]", " ", 
               RowBox[{"Cos", "[", 
                RowBox[{"\[CapitalOmega]", " ", "rmax"}], "]"}]}]}]}]}], 
           "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{"a", ",", "b"}], "}"}]}], "]"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"a", " ", "=", " ", 
       RowBox[{"a", "/.", "ns"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"b", " ", "=", " ", 
       RowBox[{"b", "/.", "ns"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       SuperscriptBox["a", "2"], "+", 
       SuperscriptBox["b", "2"]}]}]}], "\[IndentingNewLine]", "]"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.9302964012420673`*^9, 3.930296405134584*^9}, {
