Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"BWFreq", "[", 
    RowBox[{"\[CapitalOmega]2_", ",", "par_", ",", 
     RowBox[{"prec_", ":", "18"}], ",", 
     RowBox[{"search_", ":", 
      SuperscriptBox["10", 
       RowBox[{"-", "6"}]]}], ",", 
     RowBox[{"rmax_", ":", "25"}]}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "data", ",", "\[CapitalOmega]Guess", ",", "int", ",", "\[Omega]R1", ",",
        "data2", ",", "nlm", ",", "\[Omega]R", ",", "\[Omega]I"}], "}"}], ",",
      "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"\[CapitalOmega]Guess", "=", 
       RowBox[{"SetPrecision", "[", 
        RowBox[{"\[CapitalOmega]2", ",", "100"}], "]"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{"First", " ", "broad", " ", "search"}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"data", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"\[CapitalOmega]", ",", 
           RowBox[{"BreitWigner", "[", 
            RowBox[{"\[CapitalOmega]", ",", "par", ",", "rmax", ",", "prec"}],
             "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"\[CapitalOmega]", ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"1", "-", 
              SuperscriptBox["10", 
               RowBox[{"-", "2"}]]}], ")"}], " ", " ", 
            "\[CapitalOmega]Guess"}], ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"1", "+", 
              SuperscriptBox["10", 
               RowBox[{"-", "2"}]]}], ")"}], " ", " ", 
            "\[CapitalOmega]Guess"}], ",", 
           RowBox[{
            SuperscriptBox["10", 
             RowBox[{"-", "3"}]], "  ", "\[CapitalOmega]Guess"}]}], "}"}]}], 
        "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"int", "=", 
       RowBox[{"Interpolation", "[", "data", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"\[Omega]R1", "=", 
       RowBox[{
        RowBox[{"Re", "[", "x", "]"}], "/.", 
        RowBox[{"First", "@", 
         RowBox[{"Quiet", "@", 
          RowBox[{"FindRoot", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"int", "'"}], "[", "x", "]"}], "==", "0"}], ",", " ", 
            RowBox[{"{", 
             RowBox[{"x", ",", " ", "\[CapitalOmega]Guess"}], "}"}]}], 
           "]"}]}]}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"data2", " ", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"\[CapitalOmega]", ",", 
           RowBox[{"Abs", "@", 
            RowBox[{"BreitWigner", "[", 
             RowBox[{
             "\[CapitalOmega]", ",", "par", ",", "rmax", ",", "prec"}], 
             "]"}]}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"\[CapitalOmega]", ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"1", "-", "search"}], ")"}], " ", "\[Omega]R1"}], ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"1", "+", "search"}], ")"}], "\[Omega]R1"}], ",", 
           RowBox[{
            FractionBox["search", "10"], "\[Omega]R1"}]}], "}"}]}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"nlm", "=", 
       RowBox[{"LinearModelFit", "[", 
        RowBox[{"data2", ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", 
           SuperscriptBox["x", "2"]}], "}"}], ",", "x"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"\[Omega]R", "=", 
       RowBox[{
        RowBox[{"Re", "[", "x", "]"}], "/.", 
        RowBox[{"First", "@", 
         RowBox[{"Quiet", "@", 
          RowBox[{"FindRoot", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"nlm", "'"}], "[", "x", "]"}], "==", "0"}], ",", " ", 
            RowBox[{"{", 
             RowBox[{"x", ",", " ", "\[Omega]R1"}], "}"}], ",", 
            RowBox[{"AccuracyGoal", "->", "6"}]}], "]"}]}]}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"\[Omega]I", "=", 
       RowBox[{"Re", "[", 
        RowBox[{"Sqrt", "[", 
         FractionBox[
          RowBox[{"2", " ", 
           RowBox[{"nlm", "[", "\[Omega]R", "]"}]}], 
          RowBox[{
           RowBox[{"nlm", "''"}], "[", "\[Omega]R", "]"}]], "]"}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"\[Omega]R", "-", 
        RowBox[{"I", " ", "\[Omega]I"}]}], "]"}]}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}]], "Input",
